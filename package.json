{
  "name": "arweave-bundler",
  "version": "1.0.9",
  "description": "A bundler and a cache for Arweave transactions made by Point Node",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/src/index.js",
    "build": "tsc",
    "watch": "tsc -w",
    "develop": "nodemon -i '**/node_modules/**' --ext 'ts,yaml' --exec 'ts-node src/index.ts'",
    "fix": "eslint --ext .ts --fix src/",
    "lint": "eslint --ext .ts src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pointnetwork/arweave-bundler.git"
  },
  "keywords": [
    "point",
    "network",
    "arweave"
  ],
  "author": "Point Network",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pointnetwork/arweave-bundler/issues"
  },
  "homepage": "https://github.com/pointnetwork/arweave-bundler#readme",
  "dependencies": {
    "@elastic/ecs-pino-format": "^1.3.0",
    "arweave": "^1.10.17",
    "async-retry": "^1.3.3",
    "aws-sdk": "^2.1005.0",
    "busboy": "^0.2.14",
    "config": "^3.3.7",
    "ethereumjs-util": "^7.1.4",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "formidable": "^2.0.1",
    "graphql": "^16.4.0",
    "graphql-request": "^4.2.0",
    "js-yaml": "^4.1.0",
    "multer": "1.4.3",
    "multer-s3": "2.9.0",
    "pino": "^7.9.2",
    "pino-multi-stream": "^6.0.0",
    "pino-pretty": "^7.6.0",
    "pino-udp": "^1.0.1",
    "testweave-sdk": "0.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.14",
    "@types/pino-multi-stream": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
